{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["lib", "README.md"],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "private": true
  }
}