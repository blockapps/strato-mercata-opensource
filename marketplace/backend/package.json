{
  "scripts": {
    "mocha-babel": "node_modules/.bin/mocha --require @babel/register --inline-diffs --exit",
    "start": "babel-node index",
    "sitemap": "babel-node sitemap/getSitemap.js",
    "start:prod": "NODE_ENV=production babel-node index",
    "deploy": "cp config/${CONFIG:-localhost}.config.yaml ${CONFIG_DIR_PATH:-.}/config.yaml && yarn mocha-babel dapp/dapp/dapp.deploy.js --config ${CONFIG_DIR_PATH:-.}/config.yaml",
    "deploy:develop": "yarn deploy:clean && DEVELOP=true yarn deploy",
    "deploy:clean": "yarn mocha-babel dapp/dapp/dapp.clean.js",
    "deploy:secondary-org": "cp config/${CONFIG:-localhost}.config.yaml ${CONFIG_DIR_PATH:-.}/config.yaml && yarn mocha-babel dapp/dapp/dapp.deploy-org.js --config ${CONFIG_DIR_PATH:-.}/config.yaml",
    "test": "PORT=3333 yarn mocha-babel dapp/**/*.test.js && PORT=3334 yarn mocha-babel test/v1 -b",
    "test:dapp": "yarn mocha-babel dapp/dapp/test/dapp.test.js -b",
    "test:e2e": "yarn mocha-babel dapp/dapp/test/e2e.test.js -b",
    "test:category": "yarn mocha-babel dapp/categories/test -b",
    "test:product": "yarn mocha-babel dapp/products/test -b",
    "test:item": "yarn mocha-babel dapp/items/test -b",
    "test:order": "yarn mocha-babel dapp/orders/test -b",
    "test:userAddress": "yarn mocha-babel dapp/addresses/test -b",
    "test:payment": "yarn mocha-babel dapp/payments/test -b",
    "test:rawMaterial": "yarn mocha-babel dapp/items/rawMaterials/test -b",
    "test:api:category": "yarn mocha-babel test/v1/category.test.js -b",
    "test:api:subCategory": "yarn mocha-babel test/v1/subCategory.test.js -b",
    "test:api:product": "yarn mocha-babel test/v1/product.test.js -b",
    "test:api:inventory": "yarn mocha-babel test/v1/inventory.test.js -b",
    "test:api:item": "yarn mocha-babel test/v1/item.test.js -b",
    "test:api:order": "yarn mocha-babel test/v1/order.test.js -b",
    "test:api:anonymousUser": "yarn mocha-babel test/v1/anonymousUser.test.js -b",
    "test:api:image": "yarn mocha-babel test/v1/image.test.js -b",
    "test:api:marketplace": "yarn mocha-babel test/v1/marketplace.test.js -b",
    "test:api": "yarn mocha-babel test/v1/ -b",
    "addToDapp": "babel-node membership-utility/addToDapp.js",
    "clean": "rm -f ${CONFIG_DIR_PATH:-config}/*.deploy.yaml ${CONFIG_DIR_PATH:-config}/testdeploy.*"
  },
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "npm": ">=10.5.0 <11.0.0",
    "node": ">=21.7.1 <22.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.6.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "bignumber.js": "^9.1.2",
    "blockapps-rest": "8.4.0",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "clui": "^0.3.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "is-subset": "^0.1.1",
    "jpeg-js": "^0.4.4",
    "jwt-decode": "^3.1.2",
    "kafkajs": "^2.2.4",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "p-limit": "3",
    "path": "^0.12.7",
    "ramda": "^0.28.0",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "ws": "^8.13.0"
